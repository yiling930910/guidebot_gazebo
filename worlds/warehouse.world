      <uri>model://bookshelf</uri>
      <name>rack1</name>
      <pose>2 1 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://bookshelf</uri>
      <name>rack2</name>
      <pose>2 3 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://bookshelf</uri>
      <name>rack3</name>
      <pose>2 5 0 0 0 0</pose>
    </include>


    <include>
      <uri>model://cardboard_box_01</uri>
      <name>box1</name>
      <pose>3.5 1.5 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://cardboard_box_01</uri>
      <name>box2</name>
      <pose>3.5 3.5 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://cardboard_box_01</uri>
      <name>box3</name>
      <pose>3.5 5.5 0 0 0 0</pose>
    </include>

    <!-- 場景邊界（使用 construction_barrel 當臨時障礙） -->
    <include>
      <uri>model://construction_barrel</uri>
      <pose>0 6 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://construction_barrel</uri>
      <pose>4 6 0 0 0 0</pose>
    </include>

  </world>
</sdf>
